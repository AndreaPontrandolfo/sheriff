{
  "name": "sheriff-webservices",
  "description": "Sheriff webservices.",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "tsx watch ./src/index.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings=0 --cache --cache-location=node_modules/.cache/.eslintcache",

{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {},
    "start": {
      "cache": false,
      "persistent": true
    },
    "publint": {
      "outputs": ["dist/**"],
      "dependsOn": ["build"]
    },
    "clean": {
      "cache": false
    }
  }
}

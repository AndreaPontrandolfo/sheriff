{
  "name": "@sheriff/types",
  "description": "Sheriff types.",
  "version": "2.0.0",
  "license": "MIT",
  "type": "module",
  "files": [
    "src"
  ],
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "module": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"

{
  "name": "eslint-config-sheriff",
  "version": "0.0.0-development",
  "description": "A comprehensive Eslint configuration.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js"
    },
    "./recommended": "./dist/configs/eslint.config.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/index.js",
    "dist/configs"
  ],
  "author": {
    "name": "Andrea Pontrandolfo",
    "email": "andrea.pontra@gmail.com",
    "url": "https://github.com/AndreaPontrandolfo"
  },
  "repository": "https://github.com/AndreaPontrandolfo/sheriff",
  "keywords": [
    "javascript",
    "typescript",
    "eslint",
    "prettier",
    "playwright",
    "jsdoc",
    "lodash",
    "sonarjs",
    "unicorn",
    "react"
  ],
  "tsup": {
    "entry": [
      "index.ts"
    ],
    "splitting": false,
    "sourcemap": false,
    "clean": true
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "semantic-release": "semantic-release",
    "update-types": "typesync",
    "build": "tsc"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^12.3.1",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "cosmiconfig": "^7.0.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-lodash-f": "^7.5.2",
    "eslint-plugin-playwright": "^0.11.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.15.0",
    "eslint-plugin-unicorn": "^43.0.2"
  },
  "peerDependencies": {
    "typescript": ">=4.8.2"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/eslint": "^8.4.6",
    "@types/node": "^18.8.3",
    "semantic-release": "^19.0.5",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4",
    "typesync": "^0.9.2"
  }
}
